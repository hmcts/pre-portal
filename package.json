  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{js,ts}": "eslint --cache --fix"
  },
  "packageManager": "yarn@3.8.1"
}
