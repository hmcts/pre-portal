    "sass": "1.54.9",
    "sass-lint": "1.13.1",
    "sass-loader": "13.0.2",
    "sinon": "14.0.0",
    "sinon-chai": "3.7.0",
    "style-loader": "3.3.1",
    "ts-jest": "29.0.1",
    "ts-loader": "9.3.1",
    "webpack": "5.76.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-middleware": "5.3.3",
    "webpack-node-externals": "3.0.0"
  },
  "resolutions": {
    "@opentelemetry/instrumentation": "0.48.0",
    "semver": "^7.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{js,ts}": "eslint --cache --fix"
  },
  "packageManager": "yarn@3.8.0"
}
