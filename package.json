{
  "name": "pre-portal",
  "version": "0.0.2",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register src/main/server.ts",
    "start:dev": "nodemon --config nodemon.json",
    "lint": "sass-lint -v -q && eslint . --ext .js,.jsx,.ts,.tsx && prettier --check .",
    "lint:fix": "prettier --write . && eslint --fix .",
    "build": "yarn webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production yarn webpack --mode production --config webpack.config.js",
    "build:ts": "tsc -P . --outDir ./src/main",
    "test": "npx if-env CI=true && exit 0 || yarn test:unit",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:routes": "jest -c jest.routes.config.js",
    "test:a11y": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.a11y.config.js --maxWorkers 15",
    "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.smoke.config.js",
    "test:functional": "yarn playwright install && NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs run --config ./codecept.conf.js --steps",
    "cichecks": "yarn build && yarn rebuild puppeteer && yarn lint && yarn test && yarn test:routes && yarn test:a11y",
    "prepare": "husky install",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@codeceptjs/allure-legacy": "^1.0.2",
    "@hmcts/info-provider": "1.0.0",
    "@hmcts/nodejs-healthcheck": "1.7.3",
    "@hmcts/nodejs-logging": "4.0.4",
    "@hmcts/properties-volume": "0.0.14",
    "@types/body-parser": "1.19.5",
    "@types/config": "3.3.3",
    "@types/cookie-parser": "1.4.6",
    "@types/csurf": "1.11.5",
    "@types/es6-promisify": "6.0.4",
    "@types/express": "4.17.21",
    "@types/glob": "8.0.0",
    "@types/lodash": "4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "@types/nunjucks": "3.2.6",
    "@types/require-directory": "2.1.6",
    "@types/serve-favicon": "2.5.7",
    "applicationinsights": "2.3.5",
    "axios": "^1.2.0",
    "body-parser": "1.20.2",
    "compression": "^1.7.4",
    "config": "3.3.11",
    "cookie-parser": "1.4.6",
    "csurf": "1.11.0",
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "express-nunjucks": "2.2.5",
    "glob": "8.0.3",
    "govuk-frontend": "5.1.0",
    "helmet": "6.0.0",
    "jquery": "3.6.1",
    "lint": "^0.8.6",
    "lodash": "4.17.21",
    "nunjucks": "3.2.4",
    "require-directory": "2.1.1",
    "serve-favicon": "2.5.0",
    "supertest": "^6.3.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typed-rest-client": "^1.8.9",
    "typescript": "4.8.3"
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@codeceptjs/configure": "^0.10.0",
    "@jest/globals": "^29.3.1",
    "@types/chai-as-promised": "7.1.8",
    "@types/compression": "^1.7.2",
    "@types/jest": "^29.5.12",
    "@types/mini-css-extract-plugin": "2.5.1",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "axios-debug-log": "0.8.4",
    "babel-loader": "8.2.5",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "chokidar": "3.5.3",
    "codeceptjs": "^3.3.6",
    "copy-webpack-plugin": "9.1.0",
    "css-loader": "6.7.1",
    "debug": "4.3.4",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.4",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "jest-html-reporter": "3.7.0",
